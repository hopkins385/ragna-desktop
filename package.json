{
  "name": "ragna-desktop",
  "version": "0.2.6",
  "description": "RAGNA Desktop",
  "author": "Sven Stadhouders",
  "homepage": "https://ragna.app",
  "private": true,
  "license": "other",
  "main": "./out/main/index.js",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build:check": "npm run typecheck && electron-vite build",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@sentry/electron": "^5.0.0",
    "@tailwindcss/typography": "^0.5.13",
    "@tiptap/extension-bubble-menu": "^2.4.0",
    "@tiptap/extension-highlight": "^2.4.0",
    "@tiptap/extension-underline": "^2.4.0",
    "@tiptap/pm": "^2.4.0",
    "@tiptap/starter-kit": "^2.4.0",
    "@tiptap/vue-3": "^2.4.0",
    "@vee-validate/zod": "^4.13.1",
    "@vueuse/core": "^10.10.0",
    "@vueuse/electron": "^10.10.0",
    "@vueuse/integrations": "^10.10.0",
    "apache-arrow": "^14.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "easydl": "^1.2.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^9.0.0",
    "express": "^4.19.2",
    "fuse.js": "^6.6.2",
    "langchain": "^0.2.5",
    "lucide-vue-next": "^0.390.0",
    "mammoth": "^1.7.2",
    "markdown-it": "^14.1.0",
    "node-llama-cpp": "^3.0.0-beta.27",
    "pdf2json": "^3.1.3",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "radix-vue": "^1.8.3",
    "recursive-readdir": "^2.2.3",
    "sqlite3": "5.1.6",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tiptap-markdown": "^0.8.10",
    "typeorm": "^0.3.20",
    "vectordb": "^0.5.0",
    "vee-validate": "^4.13.1",
    "vue-dompurify-html": "^5.0.1",
    "vue-router": "^4.3.2",
    "vue-sonner": "^1.1.2",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@swc/core": "^1.5.29",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "autoprefixer": "^10.4.19",
    "electron": "^30.1.0",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "prettier": "^3.3.1",
    "prettier-plugin-tailwindcss": "^0.6.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-alpha.10",
    "vite": "^5.2.12",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.4.27",
    "vue-tsc": "^2.0.19"
  }
}
