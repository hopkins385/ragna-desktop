    "@tiptap/extension-highlight": "^2.6.1",
    "@tiptap/extension-underline": "^2.6.0",
    "@tiptap/pm": "^2.6.0",
    "@tiptap/starter-kit": "^2.6.0",
    "@tiptap/vue-3": "^2.6.0",
    "@vee-validate/zod": "^4.13.2",
    "@vueuse/core": "^10.11.1",
    "@vueuse/electron": "^10.11.1",
    "@vueuse/integrations": "^10.11.1",
    "@xenova/transformers": "^2.17.2",
    "apache-arrow": "^14.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "easydl": "^1.2.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.0",
    "express": "^4.19.2",
    "fuse.js": "^6.6.2",
    "langchain": "^0.2.15",
    "lucide-vue-next": "^0.427.0",
    "mammoth": "^1.8.0",
    "markdown-it": "^14.1.0",
