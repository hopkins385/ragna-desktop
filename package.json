{
  "name": "ragna-desktop",
  "version": "0.5.0",
  "description": "A powerful desktop application for document management and AI-powered chat interactions",
  "author": {
    "name": "Sven Stadhouders"
  },
  "homepage": "https://github.com/hopkins385/ragna-desktop#readme",
  "license": "MIT",
  "main": "./out/main/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hopkins385/ragna-desktop.git"
  },
  "bugs": {
    "url": "https://github.com/hopkins385/ragna-desktop/issues"
  },
  "keywords": [
    "electron",
    "vue",
    "typescript",
    "ai",
    "chat",
    "documents",
    "pdf",
    "nlp",
    "embeddings",
    "vector-search",
    "local-ai",
    "desktop-app",
    "document-management",
    "langchain",
    "transformers"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build:check": "npm run typecheck && electron-vite build",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@lancedb/lancedb": "^0.17.0",
    "@sentry/electron": "^6.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@vee-validate/zod": "^4.15.0",
    "@vueuse/core": "^12.7.0",
    "@vueuse/electron": "^12.7.0",
    "@vueuse/integrations": "^12.7.0",
    "@xenova/transformers": "^2.17.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "color-string": "^2.0.1",
    "easydl": "^1.2.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.1",
    "express": "^5.0.0",
    "fuse.js": "^7.1.0",
    "langchain": "^0.3.19",
    "langsmith": "^0.3.12",
    "lucide-vue-next": "^0.477.0",
    "markdown-it": "^14.1.0",
    "matomo-tracker": "^2.2.4",
    "node-abi": "^4.0.0",
    "node-llama-cpp": "^3.6.0",
    "officeparser": "^5.1.1",
    "pdf2json": "^3.1.5",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "radix-vue": "^1.9.17",
    "recursive-readdir": "^2.2.3",
    "sharp": "^0.33.5",
    "sqlite3": "^5.1.6",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "typeorm": "^0.3.21",
    "vee-validate": "^4.15.0",
    "vue-dompurify-html": "^5.2.0",
    "vue-router": "^4.5.0",
    "vue-sonner": "^1.3.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^2.5.0",
    "@swc/core": "^1.11.6",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "electron": "^35.0.0",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.0.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.0",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  }
}
